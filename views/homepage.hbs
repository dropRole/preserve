    <div class='cover-container d-flex w-100 h-100 p-3 mx-auto flex-column'>
      <header class='mb-auto'>
        <div>
          <h3 class='float-md-start mb-0'>pReserve</h3>
          <nav class='nav nav-masthead justify-content-center float-md-end'>
            <div class='dropdown'>
              <button
                class='btn btn-secondary dropdown-toggle'
                type='button'
                id='accountButton'
                data-bs-toggle='dropdown'
                aria-expanded='false'
              >
              </button>
              <ul
                class='dropdown-menu dropdown-menu-dark'
                aria-labelledby='accountButton'
              >
                <li><a class='dropdown-item' href='#'>Username
                    <span id='usernameUpdate'>&#9998;</span></a></li>
                <li><hr class='dropdown-divider' /></li>
                <li><a id='logout' class='dropdown-item' href='#'>Log out</a></li>
              </ul>
            </div>
          </nav>
        </div>
      </header>

      <main class='px-3'>
        <p id='estGeoMun' class='h4'>Geocoding...</p>
        <button
          id='insMdlTgglBtn'
          class='d-none'
          data-bs-toggle='modal'
          data-bs-target='#insightModal'
        ></button>
        <button
          id='srchOffrBtn'
          class='btn btn-outline-light'
          disabled
        >Search</button>
        <button id='resReqInsBtn' class='btn btn-primary'>Inspect requests</button>
        <button id='resInsBtn' class='btn btn-success'>Inspect todays
          reservations</button>
        <img
          id='complFrmImg'
          data-bs-toggle='modal'
          data-bs-target='#resComplMdl'
          src='../images/quote.png'
        />
      </main>

      {{! Generic insight modal dialog }}

      <div
        class='modal fade'
        id='insightModal'
        tabindex='-1'
        aria-labelledby='insMdlLbl'
        aria-hidden='true'
      >
        <div class='modal-dialog modal-lg'>
          <div class='modal-content'>
            <div class='modal-header'>
              <h5 class='modal-title' id='insMdlLbl'></h5>
              <button
                type='button'
                class='btn-close'
                data-bs-dismiss='modal'
                data-bs-target='#insightModal'
                aria-label='Close'
              ></button>
            </div>
            <div class='modal-body'>

            </div>
          </div>
        </div>
      </div>

      {{! Reservation request modal form dialog }}

      <div
        class='modal fade'
        id='resReqMdl'
        tabindex='-1'
        aria-labelledby='resReqMdlLbl'
        aria-hidden='true'
      >
        <div class='modal-dialog'>
          <div class='modal-content'>
            <div class='modal-header'>
              <h5 class='modal-title' id='resReqMdlLbl'>Request reservation</h5>
              <button
                type='button'
                class='btn-close'
                data-bs-dismiss='modal'
                data-bs-target='#resReqMdl'
                aria-label='Close'
              ></button>
            </div>
            <div class='modal-body'>
              <form id='reservationRequest'>
                <input type='hidden' name='idOfferors' />
                <div class='mb-3'>
                  <label for='offeror' class='col-form-label'>Offeror:</label>
                  <input
                    type='input'
                    name='offeror'
                    class='form-control'
                    disabled
                  />
                </div>
                <div class='row'>
                  <div class='col-6'>
                    <label for='date' class='col-form-label'>Date:</label>
                    <input
                      type='date'
                      name='date'
                      class='form-control'
                      id='date'
                      required
                    />
                  </div>
                  <div class='col-6'>
                    <label for='time' class='col-form-label'>Time:</label>
                    <input
                      type='time'
                      name='time'
                      class='form-control'
                      id='time'
                      required
                    />
                  </div>
                </div>
                <div class='mb-3'>
                  <label for='seats' class='col-form-label'>Seats:</label>
                  <input
                    type='number'
                    name='seats'
                    class='form-control'
                    id='seats'
                    required
                  />
                </div>
                <div class='mb-3'>
                  <label for='cause' class='col-form-label'>Cause:</label>
                  <input
                    type='input'
                    name='cause'
                    class='form-control'
                    id='cause'
                    required
                  />
                </div>
                <div class='mb-3'>
                  <label for='note' class='col-form-label'>Note:</label>
                  <textarea
                    class='form-control'
                    id='note'
                    name='note'
                    resize='none'
                  ></textarea>
                </div>
                <button
                  id='requestSubmit'
                  class='btn btn-outline-dark'
                  type='submit'
                >Request</button>
              </form>
            </div>
          </div>
        </div>
      </div>

      {{! Reservation complaint modal form dialog }}

      <div
        class='modal fade'
        id='resComplMdl'
        tabindex='-1'
        aria-labelledby='resComplMdlLbl'
        aria-hidden='true'
      >
        <div class='modal-dialog'>
          <div class='modal-content'>
            <div class='modal-header'>
              <h5 class='modal-title' id='resComplMdlLbl'>Complain on
                reservation</h5>
              <button
                type='button'
                class='btn-close'
                data-bs-dismiss='modal'
                data-bs-target='#resComplMdl'
                aria-label='Close'
              ></button>
            </div>
            <div class='modal-body'>
              <form id='resComplFrm'>
                <div class='mb-3'>
                  <label
                    for='reservation'
                    class='col-form-label'
                  >Reservation:</label>
                  <select name='idReservations' class='form-control'></select>
                </div>
                <div class='mb-3'>
                  <label for='content' class='col-form-label'>Content:</label>
                  <textarea class='form-control' name='content'></textarea>
                </div>
                <button
                  id='complFrmSbmBtn'
                  class='btn btn-outline-dark'
                  type='submit'
                >Complain</button>
              </form>
            </div>
          </div>
        </div>
      </div>

      <footer class='mt-auto text-white-50'>
